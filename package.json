    "autoprefixer": "9.5.x",
    "browser-sync": "2.26.x",
    "eslint": "^7.6.0",
    "gulp": "4.0.x",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-sass": "4.0.x",
